<!--Traemos la variable que se creo en el controlador-->
<div id="pins" class="transitions-enabled clearfix masonry mt-3">
  <% @pins.each do |pin| %>
    <div class="pin panel panel-default">
      <div class="panel-body">
        <img src="<%= pin.image_url %>" alt="<%= pin.title %>">
        <h2><%= pin.title %></h2>
        <p class="user">Publicado por <%= pin.user.email %></p>
      </div>
    </div>
  <% end %>
</div>